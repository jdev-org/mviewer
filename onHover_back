if (points.length) {
            var pointId = points[0].index;
            vectorPoint.getSource().getFeatures().forEach(function (elt) {
              var carteId = elt.getProperties().properties.id;
              if (pointId >= carteId) {
                elt.setStyle(style["selected"]);
              } else {
                elt.setStyle(style["stylePoint"]);
              }
            })
          }
