type: 'linear',
      ticks: {
        callback: function(value, index, values) {
          // Afficher l'étiquette seulement si la valeur est un multiple de 1000
          if (value % 1000 === 0) {
            return value + ' m'; // Ajouter 'm' pour indiquer que c'est en mètre
          } else {
            return ''; // Retourner une chaîne vide pour ne pas afficher l'étiquette
          }
        }
      },
